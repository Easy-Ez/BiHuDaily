<resources>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@color/statusBarColor</item>
    </style>

    <style name="AppTheme.NoActionBar.Settings">
        <item name="colorAccent">@color/settings_accent_color</item>
    </style>
    <!--过渡动画theme-->
    <style name="EdgeEffectOverlay">
        <item name="android:colorEdgeEffect">@color/edgeEffectColor</item>
    </style>
</resources>
